{
  "builds": [
    {
      "src": "fundamentals/code-quality/.vitepress/dist/**",
      "use": "@vercel/static"
    },
    {
      "src": "fundamentals/bundling/.vitepress/dist/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/fundamentals/code-quality(?:/(.*))?",
      "dest": "/fundamentals/code-quality/.vitepress/dist/$1"
    },
    {
      "src": "/fundamentals/bundling(?:/(.*))?",
      "dest": "/fundamentals/bundling/.vitepress/dist/$1"
    },
    {
      "src": "/",
      "dest": "/fundamentals/code-quality/"
    }
  ]
}