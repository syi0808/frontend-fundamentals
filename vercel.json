{
  "builds": [
    {
      "src": "dist/fundamentals/code-quality/**",
      "use": "@vercel/static"
    },
    {
      "src": "dist/fundamentals/bundling/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/fundamentals/code-quality(?:/(.*))?",
      "dest": "/dist/fundamentals/code-quality/$1"
    },
    {
      "src": "/fundamentals/bundling(?:/(.*))?",
      "dest": "/dist/fundamentals/bundling/$1"
    },
    {
      "src": "/",
      "dest": "/fundamentals/code-quality/"
    }
  ]
}