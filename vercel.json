{
  "redirects": [
    {
      "source": "/",
      "destination": "/code-quality/",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/fundamentals/code-quality(?:/(.*))?",
      "destination": "/dist/fundamentals/code-quality/$1"
    },
    {
      "source": "/fundamentals/bundling(?:/(.*))?",
      "destination": "/dist/fundamentals/bundling/$1"
    },
    {
      "source": "/",
      "destination": "/dist/fundamentals/code-quality/"
    }
  ]
}